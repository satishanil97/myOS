alias counter S0;
alias n S1;
alias i S2;
alias c S3;

print "Enter n : ";
read n;
counter = 2;

if(n >= 2) then
	print counter;
endif;

counter = 3;
while(counter <= n) do
	i = 2;
	c = 1;
	while(i <= counter/2) do
       		 if(counter%i == 0) then
               		 c = 0;
			break;
	        endif;
		i = i + 1;
	endwhile;

	if(c == 1) then
		print counter;
	endif;
        counter = counter + 1;
endwhile;

